<h1>Showing All Dogs</h1>
<div class="doge-info">
  <div class="list-inline">
    <% @dogs.each do |d| %>
      <li class="dog-col-info">
        <h2 class="dog-name"><%= link_to d.name, admin_dog_path(d) %></h2>
        <div class="dog-photo">
          <%= image_tag d.photo, height: "300", width: "350" %>
        </div>
        <h4 class="breed-name"><%= d.name %> is a <%= d.breed %></h4>
        <% if d.gender.titleize == "Female" %>
          <h4 class="dog-age">She is <%= d.age %> years old.</h4>
          <%= d.title_age.titleize %>
        <% else %>
          <h4 class="dog-age">He is <%= d.age %> years old.</h4>
          <%= d.title_age.titleize %>
        <% end %>
      </li>
    <% end %>
  </div>
</div>
<h4><%= link_to "Home Page", root_path %></h4>
<link rel="stylesheet" href="/css/custom.css">
